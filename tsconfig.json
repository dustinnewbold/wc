{
  "compilerOptions": {
    "target": "esnext",
    "jsx": "react",
    "experimentalDecorators": true,
    "jsxFactory": "WC.h",
    "module": "commonjs",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "skipLibCheck": true,
    "declaration": true
  },
  "include": [
    "./src"
  ]
}
